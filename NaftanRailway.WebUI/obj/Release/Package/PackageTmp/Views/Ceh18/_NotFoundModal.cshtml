@model string
<!--Date Modal-->
<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <h3 class="modal-title text-center" id="gridSystemModalLabel">Не найдено информации!</h3>
        </div>
        <div class="modal-body text-center">
            <img src="/Content/Images/NotFound.png" alt="Не найдено совпадений" style="width: 15em; height: 15em" />
            <div class="row text-center">@((@Model != String.Empty) ? "Нет информации по накладной № " + @Model : "Введите верный номер накладной!")</div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" data-dismiss="modal">Закрыть</button>
        </div>
    </div>
    <!-- /.modal-content -->
</div>
<!-- /.modal-dialog -->