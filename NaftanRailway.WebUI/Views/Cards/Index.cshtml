@model NaftanRailway.Domain.BusinessModels.ShippingInfoLine

@{
    Layout = null;
    int i = 0;
}
<style>
    .checkBoxList {
        max-height: 175px;
        height: auto !important;
        height: 105px;
    }
</style>

@if (Model.Cards != null && Model.Cards.Count > 0) {
    <label>@Html.DisplayNameFor(m => m.Cards)</label>
    <div class="checkBoxList" style="overflow-y: scroll; width: auto; height: auto">
            @foreach (var c in Model.Cards) {
                @Html.Partial("CardsSummary", c,
                    new ViewDataDictionary{
                        {"index", i++}
                    })
            }
    </div>
}